{
    "name": "mondje/mondje-fr",
    "description": "Le framework PHP le plus flexible qui vous propose de coder sur une belle structure professionnelle et sécurisée",
    "type": "project",
    "keywords": [
        "framework",
        "Mondje",
        "Mondje framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "N'guessan N'cho N'tamon Stéphane",
            "email": "stephane.ncho@syedevpro.com"
        }
    ],
    "repositories": [
      {
         "type": "vcs",
         "url": "https://github.com/mondje/mondje-fr" 
      }
    ],    
    "autoload":{
      "psr-4":{
        "Controller\\": "intermediary"
      }
    },
    "autoload-dev":{
      "psr-4":{
        "Model\\": "middleware"
      }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "filp/whoops": "^2.6@dev",
        "phpmailer/phpmailer": "dev-master"
    }
}
